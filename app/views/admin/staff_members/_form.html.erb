<%= markup do |m|
p = StaffMemberFormPresenter.new(f, self)
 m << p.notes
 m << p.text_field_block(:email, 'メールアドレス', size: 32)
 m << p.password_field_block(:password, 'パスワード', size: 32)
 m << p.full_name_block(:family_name, :given_name, '氏名')
 m << p.full_name_block(:family_name_kana, :given_name_kana, 'フリガナ')
 m << p.date_field_block(:start_data, '開始日')
 m << p.date_field_block(:end_data, '終了日', required: false)
 m << p.check_boxes
end %>
